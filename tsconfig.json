{
  "compileOnSave": true,

  "compilerOptions": {
    "target": "ES2023",
    "module": "commonjs",
    "sourceMap": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "noUnusedParameters": true,
    "noUnusedLocals": false,
    "noImplicitReturns": true,
    "noResolve": false,
    "moduleResolution": "node"
  }
}
